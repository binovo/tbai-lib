{
    "name": "tbai-lib",
    "version": "1.0.0",
    "description": "Tbai XML generation library for odoo POS",
    "main": "dist/tbai.js",
    "scripts": {
        "bootstrap": "npm ci",
        "type-check": "tsc --noEmit",
        "lint": "eslint '*/**/*.{js,ts}' --fix",
        "type-check:watch": "npm run type-check -- --watch",
        "build": "npm run build:types && npm run lint && npm run build:prod:js && cp node_modules/qrcode/build/qrcode.js dist/qrcode.js",
        "build:debug": "npm run build:types && npm run lint && npm run build:debug:js",
        "build:types": "tsc --emitDeclarationOnly",
        "build:prod:js": "webpack --config webpack.prod.js",
        "build:debug:js": "webpack --config webpack.dev.js",
        "test": "npm run build && env CHROME_BIN=chromium-browser karma start karma.conf.js"
    },
    "keywords": [],
    "author": "Binovo IT Human Project SL",
    "license": "AGPLv3",
    "devDependencies": {
        "@babel/cli": "^7.13.0",
        "@babel/core": "^7.13.8",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/preset-env": "^7.13.9",
        "@babel/preset-typescript": "^7.13.0",
        "@types/crc": "^3.4.0",
        "@types/jasmine": "^3.6.4",
        "@types/qrcode": "^1.4.0",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "babel-loader": "^8.2.2",
        "closure-webpack-plugin": "^2.5.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.3.1",
        "google-closure-compiler": "^20210202.0.0",
        "karma": "^5.2.3",
        "karma-chrome-launcher": "^3.1.0",
        "karma-cli": "^2.0.0",
        "karma-jasmine": "^3.3.1",
        "karma-spec-reporter": "0.0.32",
        "karma-webpack": "^4.0.2",
        "lodash": "^4.17.21",
        "prettier": "^2.2.1",
        "typescript": "^3.9.9",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.3.12",
        "webpack-merge": "^5.7.3"
    },
    "dependencies": {
        "@types/node-forge": "^0.9.7",
        "crc": "^3.8.0",
        "node-forge": "^0.10.0",
        "qrcode": "^1.5.0",
        "xadesjs": "^2.3.1",
        "xmldsigjs": "^2.1.3"
    }
}
